set-location "C:\Citrix\profiles\win7"
write-host
$file = read-host "Enter filename"
$Date = read-host "How many months from today (-No)?" 

$DT = (get-date).addmonths($date)
get-childitem | ? name -eq $file | Set-Itemproperty -name CreationTime -value $DT
